<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>FluentCsvMachine</RootNamespace>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageId>Fluent.CSV.Machine</PackageId>
    <Title>Fluent CSV Machine</Title>
    <Authors>travelr</Authors>
    <Product />
    <PackageProjectUrl></PackageProjectUrl>
    <Version>1.0.2</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/travelr/FluentCsvMachine</RepositoryUrl>
    <PackageTags>csv;csv-reader;comma;separated;delimited;parser;csv-parser;fluent</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>9937143_construction_jackhammer_repair_drill_industry_icon.png</PackageIcon>
    <Description>Fluent CSV Machine - an expressions based CSV parser</Description>
    <Copyright>travelr</Copyright>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\assets\9937143_construction_jackhammer_repair_drill_industry_icon.png">
      <Visible>false</Visible>
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="..\README.md" Visible="false">
      <Visible>false</Visible>
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>


</Project>
